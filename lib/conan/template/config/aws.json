{
  "staging": {
    "key_pairs": { 
     "keypair-name": {
       "region": "us-east-1"
     }
    },
    "ec2": { 
       "app1": {
        "region": "us-east-1",
        "monitoring": false,
        "roles": ["app", "db", "staging"],
        "flavor_id": "m1.small",
        "availability_zone": "us-east-1b",
        "groups": ["default"]
       }
    },
    "rds": { 
      "db1": {
        "region": "us-east-1",
        "flavor_id": "db.m1.small",
        "backup_retention_period": "1",
        "multi_az": false,
        "availability_zone": "us-east-1b",
        "db_security_groups": ["default"],
        "db_name": "app_production",
        "master_username": "root",
        "password": "TODO"
      }
    }, 
    "security": { 
       "ec2": { 
         "default": {
           "region": "us-east-1",
           "ports": ["22", "80"]
         }
        },
      "rds": { 
        "default": {
          "region": "us-east-1",
          "ec2_security_groups": ["default"]
        }
      } 
    }, 
    "s3": { 
      "bucket-name": { 
        "region": "us-east-1"
      }
   } 
  },
  "production": {
    "ec2": { 
       "app1": {
        "region": "us-east-1",
        "monitoring": true,
        "roles": ["app", "db", "production"],
        "flavor_id": "m1.small",
        "availability_zone": "us-east-1b",
        "groups": ["default"]
       },
       "app2": {
        "region": "us-east-1",
        "roles": ["app", "db", "production"],
        "flavor_id": "m1.small",
        "availability_zone": "us-east-1b",
        "groups": ["default"]
       }
    },
    "rds": { 
      "db1": {
        "region": "us-east-1",
        "multi_az": true,
        "flavor_id": "db.m1.small",
        "allocated_storage": 100,
        "availability_zone": "us-east-1b",
        "db_security_groups": ["default"],
        "backup_retention_period": "8",
        "db_name": "app_production",
        "master_username": "root",
        "password": "TODO"
      }
    },
    "elb": { 
     "lb1": {
        "region": "us-east-1",
        "servers": ["app1", "app2"],
        "https": "false"
     }
    },
    "security": { 
       "ec2": { 
         "default": {
           "region": "us-east-1",
           "ports": ["22", "80"]
         }
        },
      "rds": { 
        "default": {
          "region": "us-east-1",
          "ec2_security_groups": ["default"]
        }
      } 
    }, 
    "s3": { 
      "test": { 
          "region": "us-east-1"
      }
   } 
  }
}
